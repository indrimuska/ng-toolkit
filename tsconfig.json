{
    "compilerOptions": {
        "target": "es5",
        "lib": ["es2015", "dom"],
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "noImplicitAny": true,
        "baseUrl": "src",
        "paths": {
            "ng-toolkit": ["module.ts"]
        }
    }
}